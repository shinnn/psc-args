{
  "name": "psc-args",
  "version": "1.0.0",
  "description": "A list of command-line options for psc (PureScript) command",
  "repository": "shinnn/psc-args",
  "scripts": {
    "build": "node --harmony_arrow_functions --harmony_scoping build.js",
    "pretest": "jscs --esnext *.js && eslint *.js && ${npm_package_scripts_build}",
    "test": "node --harmony_arrow_functions --harmony_scoping test.js"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/psc-args/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.json"
  ],
  "keywords": [
    "list",
    "array",
    "psc",
    "purescript",
    "command",
    "option",
    "options",
    "arg",
    "args",
    "argument",
    "arguments",
    "flag",
    "cli",
    "command-line",
    "browser",
    "client-side"
  ],
  "devDependencies": {
    "array-uniq": "^1.0.2",
    "eslint": "^0.17.1",
    "istanbul": "^0.3.8",
    "istanbul-coveralls": "^1.0.2",
    "jscs": "^1.11.3",
    "purescript": "^1.0.0",
    "require-bower-files": "^2.0.0",
    "tape": "^3.5.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
